  "scripts": {
    "clean": "rimraf lib dist",
    "lint": "eslint src -f checkstyle -o reports/eslint.xml",
    "lint:boil": "npm run lint -- --max-warnings 0",
    "prebuild": "npm run clean",
    "build": "webpack --progress && babel src --out-dir lib",
    "example": "npm --prefix ./examples/$component install && npm --prefix ./examples/$component start",
    "example:login": "component=Login npm run example",
    "example:recovery": "component=Recovery npm run example",
    "example:formComponents": "component=FormComponents npm run example",
    "example:passwordConfirm": "component=PasswordConfirm npm run example",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "publish-npm": "npm version 0.1.$BUILD_NUMBER && npm publish"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/test-utils/"
    ]
  },
